{
	"applicationId": "js.neutralino.neusprite",
	"version": "0.0.1",
	"defaultMode": "window",
	"port": 0,
	"documentRoot": "/www/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"exportAuthInfo": false,
	"enableExtensions": false,
	"tokenSecurity": "one-time",
	"logging": {
		"enabled": true,
		"writeToLogFile": true
	},
	"nativeAllowList": [
		"app.*",
		"os.*",
		"window.*",
		"debug.log"
	],
	"modes": {
		"window": {
			"title": "NeuSprite",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"fullScreen": false,
			"alwaysOnTop": false,
			"icon": "/www/icons/appIcon.png",
			"enableInspector": true,
			"borderless": false,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"exitProcessOnClose": true
		}
	},
	"cli": {
		"binaryName": "NeuSprite",
		"resourcesPath": "/www/",
		"clientLibrary": "/www/js/neutralino.js",
		"binaryVersion": "4.5.0",
		"clientVersion": "3.4.0"
	}
}